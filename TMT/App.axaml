<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:TMT"
             x:Class="TMT.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>

	<Application.Resources>
		<SolidColorBrush x:Key="DarkGray" >#222222</SolidColorBrush>
		<SolidColorBrush x:Key="LightGray" >#A9A9A9</SolidColorBrush>
		<SolidColorBrush x:Key="LightBlue" >#6EC0FF</SolidColorBrush>
		<SolidColorBrush x:Key="DarkBlue" >#0B1E3F</SolidColorBrush>

	</Application.Resources>
</Application>
